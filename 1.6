# Написать функцию сортирующую
a  = list(map(int, input().split(",")))
def sortirovka(a):
    for i in range(len(a)):
        minm = i
        for x in range(i + 1, len(a)):
            if a[x] < a[minm]:
                minm = x
        a[minm], a[i] = a[i], a[minm] 
    print(a)    
sortirovka(a)
